package Product;

public class ProductDto {

	public int p_no;
	public String p_title;
	public int p_price;
	public String p_desc;
	public String p_runtime;
	public double p_rate;
	public int g_no;
	
	
	
	
	
	public ProductDto(int p_no, String p_title, int p_price, String p_desc, String p_runtime, double p_rate, int g_no) {
		this.p_no = p_no;
		this.p_title = p_title;
		this.p_price = p_price;
		this.p_desc = p_desc;
		this.p_runtime = p_runtime;
		this.p_rate = p_rate;
		this.g_no = g_no;
	}





	
	public ProductDto() {
		
	}
	
	
	



	public int getP_no() {
		return p_no;
	}




	public void setP_no(int p_no) {
		this.p_no = p_no;
	}




	public String getP_title() {
		return p_title;
	}




	public void setP_title(String p_title) {
		this.p_title = p_title;
	}




	public int getP_price() {
		return p_price;
	}




	public void setP_price(int p_price) {
		this.p_price = p_price;
	}




	public String getP_desc() {
		return p_desc;
	}




	public void setP_desc(String p_desc) {
		this.p_desc = p_desc;
	}




	public String getP_runtime() {
		return p_runtime;
	}




	public void setP_runtime(String p_runtime) {
		this.p_runtime = p_runtime;
	}




	public double getP_rate() {
		return p_rate;
	}




	public void setP_rate(double p_rate) {
		this.p_rate = p_rate;
	}




	public int getG_no() {
		return g_no;
	}




	public void setG_no(int g_no) {
		this.g_no = g_no;
	}
	
}
