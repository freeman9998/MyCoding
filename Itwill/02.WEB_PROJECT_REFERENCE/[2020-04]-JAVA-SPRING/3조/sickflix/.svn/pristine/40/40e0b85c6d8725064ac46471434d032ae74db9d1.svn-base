package com.itwill.jumun_detail;

public class JumunDetail {
	private int jd_no;
	private int j_no;
	private int p_no;
	
	public JumunDetail() {
		
	}

	public JumunDetail(int jd_no, int j_no, int p_no) {
		this.jd_no = jd_no;
		this.j_no = j_no;
		this.p_no = p_no;
	}

	public int getJd_no() {
		return jd_no;
	}

	public void setJd_no(int jd_no) {
		this.jd_no = jd_no;
	}

	public int getJ_no() {
		return j_no;
	}

	public void setJ_no(int j_no) {
		this.j_no = j_no;
	}

	public int getP_no() {
		return p_no;
	}

	public void setP_no(int p_no) {
		this.p_no = p_no;
	}
	
}
